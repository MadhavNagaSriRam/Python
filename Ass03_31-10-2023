n=int(input())
for i in range(1,n+1,+1):
    if(i%2!=0):
        print(i,end=" ")
print()
for i in range(1,n+1,+1):
    if(i%2==0):
        print(i,end=" ")
print()
for i in range(1,n+3,+1):
    if(i%2!=0):
        if(i==1):
            continue
        else:
            print(i,end=" ")
print()
for i in range(1,n+3,+1):
    if(i%2==0):
        if(i==2):
            continue
        else: 
            print(i,end=" ")
